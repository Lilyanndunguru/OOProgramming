package conditionalstatement;

/**
 *
 * @author LILIAN
 */
public class Conditional {
    int num;
    void iseven(){
        if(num%2==0){
          System.out.print("The number is EVEN\n");      
       }
        
        else {
            System.out.print("The number is ODD\n");
            
        }
        
    }
    
    
}
